# Coolify Environment Variables
# Bu dosya Coolify'da otomatik SSL için gerekli ayarları içerir

# SSL/TLS ayarları
ENABLE_SSL=true
SSL_REDIRECT=true

# Domain ayarı (Coolify'da ayarlanacak)
# DOMAIN=your-domain.com

# Nginx ayarları
NGINX_WORKER_PROCESSES=auto
NGINX_WORKER_CONNECTIONS=1024

# Cache ayarları
ENABLE_GZIP=true
CACHE_CONTROL=public